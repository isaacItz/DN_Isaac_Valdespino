@model GymManager.Web.Models.MemberListViewModel
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section HeaderRight{
<form class="d-flex">
    <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
    <button class="btn btn-outline-success" type="submit">Search</button>
</form>
}
@Model 
<div class="container">
    <h1 class="mt-5">Member List</h1>
    <p>New members of the week: @Model.NewMembersCount</p>
    @await Html.PartialAsync("~/Views/Shared/_MemberList.cshtml", Model.Members);
</div>
